package com.petLife.demo.petstore.StoringDataService;

import org.springframework.beans.factory.annotation.Autowired;
import org.springframework.stereotype.Service;

import com.petLife.demo.petstore.JpaRepository.PetRepository;
import com.petLife.demo.petstore.Models.PetModel;
@Service
public class PetDetailsService {
	@Autowired
	public PetRepository petRepo;
	
	public PetModel savePetDetails(PetModel petBody) {
		
		return petRepo.save(petBody);
	}

}
