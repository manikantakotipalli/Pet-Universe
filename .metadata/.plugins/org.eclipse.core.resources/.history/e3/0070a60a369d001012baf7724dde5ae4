package com.petLife.demo.petstore.StoringDataService;

import java.util.List;

import org.springframework.beans.factory.annotation.Autowired;
import org.springframework.data.jpa.repository.JpaRepository;
import org.springframework.stereotype.Service;

import com.petLife.demo.petstore.JpaRepository.JpaRepo;
import com.petLife.demo.petstore.Models.RegistrationModel;

@Service
public class DataSearching {
	
	@Autowired
	public JpaRepository jpaLogin;
	
	  public List<RegistrationModel>  searching(String something) {

		    return   jpaLogin.findBySomething(something);
		  
	  }

}
